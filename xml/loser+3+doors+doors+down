<input type="hidden" name="next" id="next" value="50">

<script type="text/javascript">
	function toggle() {
	    var next = document.getElementById('next')
		var el = document.getElementById(next.value + 'more');
		var total = document.getElementById('show');
		el.style.display = '';
		
		if((next.value - total.value) > 0) {
			var mlink = document.getElementById('morelink');
			mlink.style.display = 'none';
		} else {
			//window.alert(next.value);
			next.value = parseInt(next.value) + 25;
			//window.alert(next.value);
			if(next.value > 100) {
				var mlink = document.getElementById('morelink');
				mlink.style.display = 'none';				
			}
		}
		return false;
	}
</script>
